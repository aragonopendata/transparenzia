<div class="deco-cont">
  <div class="row">
    <div class="large-12 columns no-gap">
      <h3>PRESIDENCIA DEL GOBIERNO</h3>
    </div>
  </div>
</div>
<div class="body-bg">
  <div class="row" data-body>
    <div class="large-12 columns">
      <h4 class="sub-titulo"><%=raw @member.current_position.upcase %></h4>
      <h2><%= @member.name %></h2>
      <div class="large-4 columns">
        <div class="mask">
        <%= image_fu @member.photo, "341x314c", class: "img-no-scale" %>
        </div>
      <div class="large-12 columns" data-profile>
      <h5>CONTENIDOS</h5>
      <nav>
        <ul>
          <li><a href="#bio">BIOGRAFÍA</a></li>
          <li><a href="#cargos">CARGOS</a></li>
          <li><a href="#novedades">NOVEDADES</a></li>
        </ul>
      </nav>
      </div>
      </div>
      <div class="large-8 columns" data-cont>
       <h5 id="bio">BIOGRAFÍA</h5>
       <p><%=raw @member.biography %></p>
       <h5 id="cargos">CARGOS</h5>
       <p><%=raw @member.biography %></p>
       <h5 id="novedades">NOVEDADES</h5>
       <div class="noticias">
         <p><%= puts @member.inspect %></p>
         <p><%=raw @member.notes %></p>
      </div>
    </div>
  </div>
</div>